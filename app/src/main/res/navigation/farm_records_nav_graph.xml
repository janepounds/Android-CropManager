    <fragment
        android:id="@+id/addLittersFragment"
        android:name="com.myfarmnow.myfarmcrop.DailogFragments.farmrecords.livestockrecords.AddLittersFragment"
        android:label="Add Litters"
        tools:layout="@layout/fragment_add_litters" >
        <action
            android:id="@+id/action_addLittersFragment_to_littersViewFragment"
