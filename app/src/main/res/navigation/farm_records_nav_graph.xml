        android:id="@+id/addCropNotesFragment"
        android:name="com.myfarmnow.myfarmcrop.fragments.farmrecords.AddCropNotesFragment"
        android:label="fragment_add_crop_notes"
        tools:layout="@layout/fragment_add_crop_notes" >
        <action
            android:id="@+id/action_addCropNotesFragment_to_cropsNotesListFragment"
            app:popUpToInclusive="true"
            app:popUpTo="@+id/main_nav_graph"
            app:destination="@id/cropsNotesListFragment" />
