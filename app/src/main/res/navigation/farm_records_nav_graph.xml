            android:id="@+id/action_littersViewFragment_to_addLittersFragment"
            app:destination="@id/addLittersFragment" />
    </fragment>
    <fragment
        android:id="@+id/medicationsViewFragment"
        android:name="com.myfarmnow.myfarmcrop.fragments.farmrecords.liveStockRecords.MedicationsViewFragment"
        android:label="Medications"
        tools:layout="@layout/fragment_medications_view" >
        <action
            android:id="@+id/action_medicationsViewFragment_to_addMedicationFragment"
            app:destination="@id/addMedicationFragment" />
    </fragment>
    <fragment
        android:id="@+id/addBreedingStockFragment"
        android:name="com.myfarmnow.myfarmcrop.fragments.farmrecords.liveStockRecords.AddBreedingStockFragment"
        android:label="Add Breeding Stock"
        tools:layout="@layout/fragment_add_breeding_stock" />
    <fragment
        android:id="@+id/addMatingsFragment"
        android:name="com.myfarmnow.myfarmcrop.fragments.farmrecords.liveStockRecords.AddMatingsFragment"
