        android:id="@+id/cropsNotesListFragment"
        android:name="com.myfarmnow.myfarmcrop.fragments.farmrecords.CropsNotesListFragment"
        android:label="fragment_crops_notes_list"
        tools:layout="@layout/fragment_crops_notes_list" >
        <action
            android:id="@+id/action_cropsNotesListFragment_to_addCropNotesFragment"
            app:destination="@id/addCropNotesFragment" />
