        <action
            android:id="@+id/action_cropActivitiesListFragment_to_fertilizerApplicationFragment"
            app:destination="@id/fertilizerApplicationFragment" />
        <action
            android:id="@+id/action_cropActivitiesListFragment_to_cropHarvestFragment"
            app:destination="@id/cropHarvestFragment" />
