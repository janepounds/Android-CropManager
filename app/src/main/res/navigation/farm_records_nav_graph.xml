            android:id="@+id/action_livestockRecordsFragment_to_littersViewFragment"
            app:destination="@id/littersViewFragment" />
        <action
            android:id="@+id/action_livestockRecordsFragment_to_medicationsViewFragment"
            app:destination="@id/medicationsViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeFragment"
        android:name="com.myfarmnow.myfarmcrop.fragments.farmrecords.StoreFragment"
        android:label="@string/label_store"
