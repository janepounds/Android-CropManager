<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyToolbarWallet" parent="AppTheme">
        <item name="android:background">@color/white</item>
        <item name="android:textColorPrimary">@color/colorAccentGreen</item>
        <item name="android:textColorSecondary">@color/colorAccentGreen</item>
        <item name="android:font">@font/montserrat</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorAccentGreen</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:font">@font/montserrat</item>
        <item name="preferenceTheme">@style/PreferenceThemeOverlay.v14.Material</item>
    </style>

    <style name="Button_DW" parent="Base.TextAppearance.AppCompat.Button">
        <item name="backgroundTint">@color/colorPrimary</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:font">@font/montserrat</item>
        <item name="android:textSize">14sp</item>

    </style>



</resources>